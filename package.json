{
  "name": "urbanmix",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "env-cmd -f .env.development vite",
    "dev:prod": "env-cmd -f .env.production vite",
    "build": "env-cmd -f .env.development tsc && env-cmd -f .env.development vite build",
    "build:prod": "env-cmd -f .env.production tsc && env-cmd -f .env.production vite build",
    "preview": "env-cmd -f .env.development vite preview",
    "lint": "eslint --ext .js,.cjs,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.cjs,.ts,.tsx . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@react-three/drei": "^9.88.7",
    "@react-three/fiber": "^8.15.8",
    "@reduxjs/toolkit": "^2.0.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/three": "^0.158.0",
    "axios": "^1.6.2",
    "env-cmd": "^10.1.0",
    "firebase": "^10.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1",
    "redux": "^5.0.0",
    "redux-persist": "^6.0.0",
